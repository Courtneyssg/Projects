   "source": [
    "\n",
    "@interact(phd= widgets.Dropdown(options=params, value=params[0], description='Y-axis:'), \n",
    "          Female = True, Male = True,\n",
    "          allphd = widgets.Checkbox(value=False, disabled = False, description=\"All Ph.D.s\", ),\n",
    "          filters = widgets.ToggleButtons(options=['Ascending % of Females', 'Region'], description='Order By: '))\n",
    "\n",
    "def f(phd, Female, Male, allphd, filters):\n",
    "    xvalue = []\n",
    "    yvalue = []\n",
    "    datavalue = []\n",
    "    sizevalue = []\n",
    "    huevalue = []\n",
    "    minsizevalue = []\n",
    "    maxsizevalue = []\n",
    "    factor = 0\n",
    "    df = dfs[phd]\n",
    "    show = True;\n",
    "    if filters == 'Ascending % of Females':\n",
    "        dft = df.sort_values(by= (phd + ' F %'), ascending=True)\n",
    "        df_filtered_percent = df.filter(['Country', 'Region', (phd + ' F %'), (phd + ' M %'), (phd + ' F'), (phd + ' M')], axis=1)\n",
    "        df_filtered_percent = df_filtered_percent.sort_values(by= (phd + ' F %'), ascending = True)\n",
    "    if filters == 'Region':\n",
    "        df_filtered_percent = df.filter(['Country', 'Region', (phd + ' F %'), (phd + ' M %'), (phd + ' F'), (phd + ' M')], axis=1)\n",
    "        df_filtered_percent = df_filtered_percent.sort_values(by= 'Region', ascending = True)\n",
    "    if not Female and not Male:\n",
    "        fig, ax = plt.subplots(figsize=(14,9))\n",
    "        ax = sns.scatterplot()\n",
    "        plt.show()\n",
    "        \n",
    "    if Female:\n",
    "        df_female_percent = df_filtered_percent.filter(['Country', (phd + ' F %'), (phd + ' F')], axis=1).reset_index(drop = True)\n",
    "        xvalue = df_female_percent.Country.copy()\n",
    "        yvalue = df_female_percent[phd + ' F %'].copy()\n",
    "        datavalue = df_female_percent.copy()\n",
    "        sizevalue = df_female_percent[phd + ' F'].copy()\n",
    "        df_female_percent['Gender'] = sizevalue.copy()\n",
    "        df_female_percent['Gender'] = 'female'\n",
    "        huevalue = df_female_percent['Gender'].copy()\n",
    "        minsizevalue = min(df_female_percent[phd + ' F'])\n",
    "        maxsizevalue = max(df_female_percent[phd + ' F'])\n",
    "        factor = 20\n",
    "        \n",
    "    if Male:\n",
    "        df_male_percent = df_filtered_percent.filter(['Country', (phd + ' M %'), (phd + ' M')], axis=1).reset_index(drop = True)\n",
    "        xvalue = df_male_percent.Country.copy()\n",
    "        yvalue = df_male_percent[phd + ' M %'].copy()\n",
    "        datavalue = df_male_percent.copy()\n",
    "        sizevalue = df_male_percent[phd + ' M'].copy()\n",
    "        df_male_percent['Gender'] = sizevalue.copy()\n",
    "        df_male_percent['Gender'] = 'male'\n",
    "        huevalue = df_male_percent['Gender'].copy()\n",
    "        minsizevalue = min(df_male_percent[phd + ' M'])\n",
    "        maxsizevalue = max(df_male_percent[phd + ' M'])\n",
    "        factor = 20\n",
    "\n",
    "            \n",
    "    if Male & Female:\n",
    "        xvalue = pd.concat([df_female_percent['Country'], df_male_percent['Country']], axis = 0).reset_index(drop = True)\n",
    "        yvalue = pd.concat([df_female_percent[phd + ' F %'], df_male_percent[phd + ' M %']], axis = 0).reset_index(drop = True)\n",
    "        sizevalue = pd.concat([df_female_percent[phd + ' F'], df_male_percent[phd + ' M']], axis = 0).reset_index(drop = True)\n",
    "        datavalue3 = pd.concat([xvalue, yvalue, sizevalue], axis = 1).reset_index(drop = True)\n",
    "        datavalue3.columns=['Country', phd + ' %', phd + ' Total']\n",
    "        huevalue = pd.concat([df_female_percent['Gender'], df_male_percent['Gender']], axis = 0).reset_index(drop = True)\n",
    "        minsizevalue = min(df_female_percent[phd + ' F']) + min(df_male_percent[phd + ' M'])\n",
    "        maxsizevalue = max(df_female_percent[phd + ' F']) + max(df_male_percent[phd + ' M'])\n",
    "        factor = 40\n",
    "\n",
    "    \n",
    "\n",
    "    fig, ax = plt.subplots(figsize=(14,9))\n",
    "    ax = sns.scatterplot(x=xvalue, y=yvalue, data=datavalue, hue = huevalue, size=sizevalue, sizes=(minsizevalue/factor, maxsizevalue/factor), ax=ax)\n",
    "    plt.legend(['Setosa', 'Versicolor', 'Virginica'])\n",
    "    plt.xticks(rotation='vertical')\n",
    "    ax.set_xlabel('')\n",
    "    ax.set_ylabel('Percent of Total')\n",
    "    plt.title('Global Ph.D.s Gender Gap (2010)')\n",
    "    ax.set_ylim((0, 100))\n",
    "    plt.show()"
   ]
